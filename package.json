{
  "name": "wolf-studio",
  "version": "0.1.0",
  "description": "Wolf Studio - Design and workplace consultancy website",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "playwright test --project=chromium",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:debug": "playwright test --debug",
    "test:lighthouse": "lhci autorun",
    "test:lighthouse:desktop": "lhci autorun --collect.settings.emulatedFormFactor=desktop",
    "test:lighthouse:mobile": "lhci autorun --collect.settings.emulatedFormFactor=mobile",
    "test:all": "npm run test:ci && npm run test:playwright && npm run test:e2e && npm run test:lighthouse",
    "test:setup": "playwright install && cypress install",
    "test:clean": "rm -rf coverage cypress/screenshots cypress/videos playwright-report test-results",
    "test:security": "npm audit && npm run test:lighthouse",
    "test:performance": "npm run test:lighthouse:desktop && npm run test:lighthouse:mobile"
  },
